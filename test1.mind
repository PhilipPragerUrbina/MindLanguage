//set a to 0 and add to b
function add : a b {
while a [
    a --
    b ++
]
}

//copy a to b leaving a the same
function copy : a b {
//set b to 0
while b [
    b --
]

//copy a to b and c
while a [
    a --
    b ++
    c ++
]
//copy c to a
while c [
    a ++
    c --
]


}

